<Window x:Class="EM_Lab_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EM_Lab_1"
        mc:Ignorable="d"
        Title="Емпіричні методи: лабораторна робота 1" Height="600" Width="960">
    <Grid>
        <TabControl>
            <TabItem x:Name="MainTab" Header="Головна" FontSize="20">
                <Grid Background="#FFE5E5E5">
                    <Button x:Name="LoadDataButton" Content="Завантажити дані" HorizontalAlignment="Center" VerticalAlignment="Center" Height="229" Width="734" FontSize="25" Click="LoadDataButtonClickHandler"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="OneSelectionTab" Header="Одна вибірка" FontSize="20" Height="31" VerticalAlignment="Top">
                <Grid Background="#FFE5E5E5">
                    <ComboBox HorizontalAlignment="Left" Margin="10,11,0,0" VerticalAlignment="Top" Width="183" SelectedIndex="0" Height="40">
                        <ComboBoxItem Content="Перша вибірка"/>
                        <ComboBoxItem Content="Друга вибірка"/>
                    </ComboBox>
                    <Image HorizontalAlignment="Right" Height="345" VerticalAlignment="Top" Width="934" Source="/Other/Images/table.png" Margin="0,75,10,0"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,440,355" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Bottom" Width="280" Height="57" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    <TextBox HorizontalAlignment="Left" Margin="234,190,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="280" Height="57" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Left" Margin="234,247,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="280" Height="58" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Left" Margin="234,305,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="280" Height="57" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Left" Margin="234,362,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="280" Height="57" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Left" Margin="514,133,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="429" Height="57" IsReadOnly="True" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}"/>
                    <TextBox HorizontalAlignment="Left" Margin="514,190,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="429" Height="57" IsReadOnly="True" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}"/>
                    <TextBox HorizontalAlignment="Left" Margin="514,247,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="429" Height="58" IsReadOnly="True" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}"/>
                    <TextBox HorizontalAlignment="Left" Margin="514,305,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="429" Height="57" IsReadOnly="True" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}"/>
                    <TextBox HorizontalAlignment="Left" Margin="514,362,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="429" Height="57" IsReadOnly="True" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,440,412" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Bottom" Width="280" Height="58" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,11,412" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Bottom" Width="429" Height="58" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox Margin="11,75,0,0" TextWrapping="Wrap" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="58" VerticalAlignment="Top" HorizontalAlignment="Left" Width="223" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" CaretBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,720,355" TextWrapping="Wrap" Text="Середнє арифметичне" VerticalAlignment="Bottom" Width="223" Height="57" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,440,355" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Bottom" Width="280" Height="57" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,720,298" TextWrapping="Wrap" Text="Медіана" VerticalAlignment="Bottom" Width="223" Height="57" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}" CaretBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,720,240" TextWrapping="Wrap" Text="Середньоквадратичне відхилення" VerticalAlignment="Bottom" Width="223" Height="58" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,720,183" TextWrapping="Wrap" Text="Коефіцієнт ексцесу" VerticalAlignment="Bottom" Width="223" Height="57" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
                    <TextBox HorizontalAlignment="Right" Margin="0,0,720,126" TextWrapping="Wrap" Text="Коефіцієнт асиметрії" VerticalAlignment="Bottom" Width="223" Height="57" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="BothSelectionsTab" Header="Обидві вибірки" FontSize="20">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
